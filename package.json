{
  "name": "starter-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm migration:up && wds --watch ./src/index.ts | pnpm pino-pretty",
    "dev:worker": "wds --watch ./src/worker.ts | pnpm pino-pretty",
    "start": "node ./dist/src/index.js",
    "start:worker": "node ./dist/src/worker.js",
    "build": "pnpm run build:prisma && pnpm run build:src",
    "build:prisma": "(mkdir -p dist/prisma && pnpm run generate) | gnomon",
    "build:src": "(mkdir -p dist && tsc && tsc-alias && echo 'build done !') | gnomon",
    "lint": "eslint src --ext .ts --fix",
    "generate": "prisma generate",
    "migration:up": "prisma migrate deploy",
    "migration:make": "prisma migrate dev",
    "migration:reset": "prisma migrate reset",
    "migration:seed": "prisma db seed",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "script:hash": "pnpm wds scripts/generate-hash.ts",
    "update-template": "update-template https://github.com/flexper/starter-api",
    "test": "rm -rf dist && jest --forceExit",
    "dev:test": "rm -rf dist && jest --watchAll"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "prisma": {
    "seed": "wds prisma/seed.ts"
  },
  "pnpm": {
    "overrides": {
      "type-graphql": "2.0.0-beta.1"
    }
  },
  "author": "Flexper",
  "license": "ISC",
  "devDependencies": {
